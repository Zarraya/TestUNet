<UserControl
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
  xmlns:noesis="clr-namespace:NoesisGUIExtensions">
	<UserControl.Resources>
		<Color x:Key="Player2BgLColor">#FFFFFF64</Color>
		<Color x:Key="Player2BgRColor">#FF998000</Color>
		<Color x:Key="Player2BdColor">#FF948000</Color>
		<Color x:Key="Player1BgLColor">#FF00FFF3</Color>
		<Color x:Key="Player1BgRColor">#FF008899</Color>
		<Color x:Key="Player1BdColor">#FF006B80</Color>
		<Color x:Key="WinColor">#FFFF3300</Color>
		<ControlTemplate x:Key="BtnTemplate" TargetType="ToggleButton">
			<Grid Background="Transparent">
				<VisualStateManager.VisualStateGroups>
					<VisualStateGroup x:Name="CommonStates">
						<VisualStateGroup.Transitions>
							<VisualTransition From="Normal" GeneratedDuration="0:0:0.2" To="MouseOver">
								<Storyboard>
									<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Over">
										<EasingDoubleKeyFrame KeyTime="0" Value="0.85"/>
										<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
											<EasingDoubleKeyFrame.EasingFunction>
												<ElasticEase EasingMode="EaseOut" Oscillations="2" Springiness="2"/>
											</EasingDoubleKeyFrame.EasingFunction>
										</EasingDoubleKeyFrame>
									</DoubleAnimationUsingKeyFrames>
									<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Over">
										<EasingDoubleKeyFrame KeyTime="0" Value="0.85"/>
										<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
											<EasingDoubleKeyFrame.EasingFunction>
												<ElasticEase EasingMode="EaseOut" Oscillations="2" Springiness="2"/>
											</EasingDoubleKeyFrame.EasingFunction>
										</EasingDoubleKeyFrame>
									</DoubleAnimationUsingKeyFrames>
								</Storyboard>
							</VisualTransition>
							<VisualTransition From="MouseOver" GeneratedDuration="0:0:0.1" To="Normal"/>
							<VisualTransition From="MouseOver" GeneratedDuration="0:0:0.1" To="Pressed">
								<Storyboard>
									<DoubleAnimation Duration="0:0:0.1" To="0.75" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Over">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseOut"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
									<DoubleAnimation Duration="0:0:0.1" To="0.75" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Over">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseOut"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
								</Storyboard>
							</VisualTransition>
							<VisualTransition From="Pressed" GeneratedDuration="0:0:0.2">
								<Storyboard>
									<DoubleAnimation Duration="0:0:0.2" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Over">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseInOut"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
									<DoubleAnimation Duration="0:0:0.2" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Over">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseInOut" Amplitude="2"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
								</Storyboard>
							</VisualTransition>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Normal"/>
						<VisualState x:Name="MouseOver">
							<Storyboard>
								<DoubleAnimation Duration="0" To="0.25" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CircleOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.25" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CircleOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.25" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CrossOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.25" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CrossOver" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Pressed">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.75"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.75"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimation Duration="0" To="0.3" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CrossOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.3" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CrossOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.3" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CircleOver" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.3" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CircleOver" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Disabled"/>
					</VisualStateGroup>
					<VisualStateGroup x:Name="FocusStates"/>
					<VisualStateGroup x:Name="CheckStates">
						<VisualStateGroup.Transitions>
							<VisualTransition From="Unchecked" GeneratedDuration="0:0:0.1" To="Checked">
								<Storyboard>
									<DoubleAnimation Duration="0:0:0.1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Check">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseOut"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
									<DoubleAnimation Duration="0:0:0.1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Check">
										<DoubleAnimation.EasingFunction>
											<BackEase EasingMode="EaseOut"/>
										</DoubleAnimation.EasingFunction>
									</DoubleAnimation>
								</Storyboard>
							</VisualTransition>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Checked">
							<Storyboard>
								<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CrossCheck" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CrossCheck" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(Shape.Stroke).(Brush.Opacity)" Storyboard.TargetName="CircleCheck" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="CircleCheck" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Unchecked">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Check">
									<EasingDoubleKeyFrame KeyTime="0" Value="0.75"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Check">
									<EasingDoubleKeyFrame KeyTime="0" Value="0.75"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Indeterminate"/>
					</VisualStateGroup>
					<VisualStateGroup x:Name="ValidationStates"/>
					<VisualStateGroup x:Name="PlayerStates">
						<VisualState x:Name="Player1">
							<Storyboard>
								<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="CrossOver">
									<DiscreteObjectKeyFrame KeyTime="0">
										<DiscreteObjectKeyFrame.Value>
											<Visibility>Collapsed</Visibility>
										</DiscreteObjectKeyFrame.Value>
									</DiscreteObjectKeyFrame>
								</ObjectAnimationUsingKeyFrames>
								<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="CrossCheck">
									<DiscreteObjectKeyFrame KeyTime="0">
										<DiscreteObjectKeyFrame.Value>
											<Visibility>Collapsed</Visibility>
										</DiscreteObjectKeyFrame.Value>
									</DiscreteObjectKeyFrame>
								</ObjectAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Player2">
							<Storyboard>
								<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="CircleOver">
									<DiscreteObjectKeyFrame KeyTime="0">
										<DiscreteObjectKeyFrame.Value>
											<Visibility>Collapsed</Visibility>
										</DiscreteObjectKeyFrame.Value>
									</DiscreteObjectKeyFrame>
								</ObjectAnimationUsingKeyFrames>
								<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="CircleCheck">
									<DiscreteObjectKeyFrame KeyTime="0">
										<DiscreteObjectKeyFrame.Value>
											<Visibility>Collapsed</Visibility>
										</DiscreteObjectKeyFrame.Value>
									</DiscreteObjectKeyFrame>
								</ObjectAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
					</VisualStateGroup>
				</VisualStateManager.VisualStateGroups>
				<Grid x:Name="Over" Margin="10" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Path x:Name="CircleOver" Data="M0,20 A20,20 0 1 0 20,0 A20,20 0 1 0 40,20 A20,20 0 1 0 20,40 A20,20 0 1 0 0,20 M6,20 A14,14 0 1 0 20,6 A14,14 0 1 0 34,20 A14,14 0 1 0 20,34 A14,14 0 1 0 6,20" Stretch="Fill" UseLayoutRounding="False" StrokeThickness="1.5">
						<Path.Stroke>
							<SolidColorBrush Color="{StaticResource Player1BdColor}" Opacity="0"/>
						</Path.Stroke>
						<Path.Fill>
							<LinearGradientBrush EndPoint="0,1" Opacity="0">
								<GradientStop Color="{StaticResource Player1BgLColor}" Offset="0.2"/>
								<GradientStop Color="{StaticResource Player1BgRColor}" Offset="1"/>
							</LinearGradientBrush>
						</Path.Fill>
					</Path>
					<Path x:Name="CrossOver" Data="M3.7207031,6.6988277 L4.5,6.6988277 L4.5,9.0601559 L6.8671875,9.0601559 L6.8671875,9.8453121 L4.5,9.8453121 L4.5,12.20664 L3.7207031,12.20664 L3.7207031,9.8453121 L1.359375,9.8453121 L1.359375,9.0601559 L3.7207031,9.0601559 z" RenderTransformOrigin="0.5,0.5" Stretch="Fill" VerticalAlignment="Stretch" StrokeThickness="1.5" >
						<Path.Stroke>
							<SolidColorBrush Color="{StaticResource Player2BdColor}" Opacity="0"/>
						</Path.Stroke>
						<Path.Fill>
							<LinearGradientBrush EndPoint="1,1" StartPoint="0,0" Opacity="0">
								<GradientStop Color="{StaticResource Player2BgLColor}" Offset="0.4"/>
								<GradientStop Color="{StaticResource Player2BgRColor}" Offset="0.8"/>
							</LinearGradientBrush>
						</Path.Fill>
						<Path.RenderTransform>
							<CompositeTransform Rotation="45"/>
						</Path.RenderTransform>
					</Path>
				</Grid>
				<Grid x:Name="Check" Margin="10" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Path x:Name="CircleCheck" Data="M0,20 A20,20 0 1 0 20,0 A20,20 0 1 0 40,20 A20,20 0 1 0 20,40 A20,20 0 1 0 0,20 M6,20 A14,14 0 1 0 20,6 A14,14 0 1 0 34,20 A14,14 0 1 0 20,34 A14,14 0 1 0 6,20" Stretch="Fill" UseLayoutRounding="False" StrokeThickness="1.5">
						<Path.Stroke>
							<SolidColorBrush Color="{StaticResource Player1BdColor}" Opacity="0"/>
						</Path.Stroke>
						<Path.Fill>
							<LinearGradientBrush EndPoint="0,1" Opacity="0">
								<GradientStop Color="{StaticResource Player1BgLColor}" Offset="0.2"/>
								<GradientStop Color="{StaticResource Player1BgRColor}" Offset="1"/>
							</LinearGradientBrush>
						</Path.Fill>
					</Path>
					<Path x:Name="CrossCheck" Data="M3.7207031,6.6988277 L4.5,6.6988277 L4.5,9.0601559 L6.8671875,9.0601559 L6.8671875,9.8453121 L4.5,9.8453121 L4.5,12.20664 L3.7207031,12.20664 L3.7207031,9.8453121 L1.359375,9.8453121 L1.359375,9.0601559 L3.7207031,9.0601559 z" RenderTransformOrigin="0.5,0.5" Stretch="Fill" VerticalAlignment="Stretch" StrokeThickness="1.5" >
						<Path.Stroke>
							<SolidColorBrush Color="{StaticResource Player2BdColor}" Opacity="0"/>
						</Path.Stroke>
						<Path.Fill>
							<LinearGradientBrush EndPoint="1,1" StartPoint="0,0" Opacity="0">
								<GradientStop Color="{StaticResource Player2BgLColor}" Offset="0.4"/>
								<GradientStop Color="{StaticResource Player2BgRColor}" Offset="0.8"/>
							</LinearGradientBrush>
						</Path.Fill>
						<Path.RenderTransform>
							<CompositeTransform Rotation="45"/>
						</Path.RenderTransform>
					</Path>
				</Grid>
			</Grid>
		</ControlTemplate>
		<Style x:Key="BtnStyle" TargetType="ToggleButton">
			<Setter Property="Margin" Value="15"/>
			<Setter Property="Template" Value="{StaticResource BtnTemplate}"/>
		</Style>
		<Storyboard x:Key="WinAnim">
			<DoubleAnimationUsingKeyFrames x:Name="WinAnim0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinRow2">
				<EasingDoubleKeyFrame KeyTime="0" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames x:Name="WinAnim1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="WinRow2">
				<EasingDoubleKeyFrame KeyTime="0:0:0" Value="0.05"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<CubicEase EasingMode="EaseIn"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames x:Name="WinAnim2" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="WinRow2">
				<EasingDoubleKeyFrame KeyTime="0:0:0" Value="0.05"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<CubicEase EasingMode="EaseIn"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Board">
				<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.05">
					<EasingDoubleKeyFrame.EasingFunction>
						<SineEase EasingMode="EaseInOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
				<EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<BackEase EasingMode="EaseOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Board">
				<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.05">
					<EasingDoubleKeyFrame.EasingFunction>
						<SineEase EasingMode="EaseInOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
				<EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<BackEase EasingMode="EaseOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
		</Storyboard>
		<Storyboard x:Key="TieAnim">
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Board">
				<EasingDoubleKeyFrame KeyTime="0:0:0.05" Value="1.05"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.15" Value="1.1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.25" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<CircleEase EasingMode="EaseOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Board">
				<EasingDoubleKeyFrame KeyTime="0:0:0.05" Value="1.05"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.15" Value="1.1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.25" Value="1">
					<EasingDoubleKeyFrame.EasingFunction>
						<CircleEase EasingMode="EaseOut"/>
					</EasingDoubleKeyFrame.EasingFunction>
				</EasingDoubleKeyFrame>
			</DoubleAnimationUsingKeyFrames>
			<ColorAnimation Duration="0:0:0.1" To="#FFFF3300" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridL" d:IsOptimized="True"/>
			<ColorAnimation Duration="0:0:0.1" To="#FFFF3300" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridR" d:IsOptimized="True"/>
			<ColorAnimation Duration="0:0:0.1" To="#FFFF3300" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridT" d:IsOptimized="True"/>
			<ColorAnimation Duration="0:0:0.1" To="#FFFF3300" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridB" d:IsOptimized="True"/>
		</Storyboard>
		<Storyboard x:Key="ResetAnim" SpeedRatio="1.5">
			<DoubleAnimation Duration="0:0:0.5" To="180" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationY)" Storyboard.TargetName="Board" d:IsOptimized="True"/>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.GlobalOffsetZ)" Storyboard.TargetName="Board">
				<EasingDoubleKeyFrame KeyTime="0:0:0.25" Value="-200"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0"/>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell00" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell01" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell02" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell10" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell11" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell12" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell20" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell21" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Cell22" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinRow1" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinRow2" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinRow3" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinCol1" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinCol2" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinCol3" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinDiagLR" d:IsOptimized="True"/>
			<DoubleAnimation Duration="0" BeginTime="0:0:0.25" To="0" Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="WinDiagRL" d:IsOptimized="True"/>
			<ColorAnimation Duration="0" BeginTime="0:0:0.25" To="White" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridL" d:IsOptimized="True"/>
			<ColorAnimation Duration="0" BeginTime="0:0:0.25" To="White" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridR" d:IsOptimized="True"/>
			<ColorAnimation Duration="0" BeginTime="0:0:0.25" To="White" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridT" d:IsOptimized="True"/>
			<ColorAnimation Duration="0" BeginTime="0:0:0.25" To="White" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GridB" d:IsOptimized="True"/>
		</Storyboard>
		<Storyboard x:Name="BgAnim">
			<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Border.Background).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bg" RepeatBehavior="Forever">
				<EasingColorKeyFrame KeyTime="0:0:2" Value="#FF46869E"/>
				<EasingColorKeyFrame KeyTime="0:0:4" Value="#FF466A9E"/>
				<EasingColorKeyFrame KeyTime="0:0:6" Value="#FF46569E"/>
				<EasingColorKeyFrame KeyTime="0:0:8" Value="#FF466D9E"/>
			</ColorAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Border.Background).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="Bg" RepeatBehavior="Forever">
