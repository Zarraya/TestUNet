<UserControl
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:noesis="clr-namespace:NoesisGUIExtensions" xmlns:System="clr-namespace:System;assembly=mscorlib" mc:Ignorable="d" Foreground="White">
	
	<UserControl.Resources>	
		<RadialGradientBrush x:Key="RedCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="Red"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="GreenCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FF158000"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="BlueCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FF0055FF"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="YellowCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="Yellow"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="BrownCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FFFF8000"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="PurpleCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FF9B009B"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="OrangeCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FF666666"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="PinkCircleBrush" Center="0.3,0.3" GradientOrigin="0.3,0.3" RadiusY="0.5" RadiusX="0.5">
			<GradientStop Offset="-0.25" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FFFF92A5"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="BlackCircleBrush" GradientOrigin="0.3,0.3" Center="0.3,0.3">
			<GradientStop Offset="-0.95" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="Black"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="WhiteCircleBrush" GradientOrigin="0.3,0.3" Center="0.3,0.3">
			<GradientStop Offset="0" Color="#FFFFFFFF"/>
			<GradientStop Offset="1" Color="#FFC1C1C1"/>
		</RadialGradientBrush>
		<RadialGradientBrush x:Key="SolutionBrush" GradientOrigin="0.3,0.3" Center="0.3,0.3">
			<GradientStop Color="#FFA8D6ED" Offset="0"/>
			<GradientStop Color="#FF2997DE" Offset="1"/>
		</RadialGradientBrush>
		<SolidColorBrush x:Key="BlackColorBrush" Color="#FF161F29"/>
		<RadialGradientBrush x:Key="CircleCheck" >
			<GradientStop Color="#FF1F3547" Offset="0.815"/>
			<GradientStop Color="#001F3547" Offset="1"/>
			<GradientStop Color="#FF1F3547" Offset="0.919"/>
		</RadialGradientBrush>
		<LinearGradientBrush x:Key="CircleIndent" EndPoint="0.691,0.962" StartPoint="0.309,0.038">
			<GradientStop Color="#4C000000" Offset="0"/>
			<GradientStop Color="#4CFFFFFF" Offset="1"/>
		</LinearGradientBrush>
		<LinearGradientBrush x:Key="ButtonBorderBrush" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.9">
			<GradientStop Color="#FF112844"/>
			<GradientStop Color="#FF243242" Offset="1"/>
		</LinearGradientBrush>
		<LinearGradientBrush x:Key="CurrentRowBrush" EndPoint="1,0" StartPoint="0,0" Opacity="0.7">
			<GradientStop Color="#00466C9D" Offset="0"/>
			<GradientStop Color="#3353A5F0" Offset="0.15"/>
			<GradientStop Color="#3353A5F0" Offset="0.85"/>
			<GradientStop Offset="1" Color="#00466C9D"/>
		</LinearGradientBrush>
		<LinearGradientBrush x:Key="CurrentRowMaskBrush" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="Black" Offset="0.4"/>
			<GradientStop Offset="0.95"/>
			<GradientStop Offset="0.05"/>
			<GradientStop Color="Black" Offset="0.6"/>
		</LinearGradientBrush>
		
		<ControlTemplate x:Key="RBTemplate" TargetType="RadioButton">
			<Grid Background="Transparent" Margin="5" Width="30" Height="30">
				<VisualStateManager.VisualStateGroups>
					<VisualStateGroup x:Name="CommonStates">
						<VisualStateGroup.Transitions>
							<VisualTransition GeneratedDuration="0:0:0.1" To="Pressed"/>
							<VisualTransition From="Pressed" GeneratedDuration="0:0:0.2"/>
							<VisualTransition From="Normal" GeneratedDuration="0:0:0.1" To="MouseOver"/>
							<VisualTransition From="MouseOver" GeneratedDuration="0:0:0.2" To="Normal"/>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Normal">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="MouseOver">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
								<ColorAnimation Duration="0" To="#33FFFFFF" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="OverHi" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Pressed">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.85"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Over">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.85"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimation Duration="0" To="0.85" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Check" d:IsOptimized="True"/>
								<DoubleAnimation Duration="0" To="0.85" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Check" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Disabled"/>
					</VisualStateGroup>
					<VisualStateGroup x:Name="FocusStates"/>
					<VisualStateGroup x:Name="CheckStates">
						<VisualStateGroup.Transitions>
							<VisualTransition From="Unchecked" GeneratedDuration="0:0:0.1" To="Checked"/>
							<VisualTransition From="Checked" GeneratedDuration="0:0:0.2" To="Unchecked"/>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Checked">
							<Storyboard>
								<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Check" d:IsOptimized="True"/>
								<Storyboard Duration="0:0:3" RepeatBehavior="Forever">
									<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="CheckShine">
										<EasingColorKeyFrame KeyTime="0" Value="#7F3A648F"/>
										<EasingColorKeyFrame KeyTime="0:0:1.5" Value="#003A648F">
											<EasingColorKeyFrame.EasingFunction>
												<ExponentialEase EasingMode="EaseIn"/>
											</EasingColorKeyFrame.EasingFunction>
										</EasingColorKeyFrame>
										<EasingColorKeyFrame KeyTime="0:0:2" Value="#003A648F"/>
										<EasingColorKeyFrame KeyTime="0:0:3" Value="#7F3A648F">
											<EasingColorKeyFrame.EasingFunction>
												<ExponentialEase EasingMode="EaseInOut" Exponent="3"/>
											</EasingColorKeyFrame.EasingFunction>
										</EasingColorKeyFrame>
									</ColorAnimationUsingKeyFrames>
									<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[2].(GradientStop.Color)" Storyboard.TargetName="CheckShine">
										<EasingColorKeyFrame KeyTime="0" Value="#4C6E97C1"/>
										<EasingColorKeyFrame KeyTime="0:0:1.5" Value="#006E97C1">
											<EasingColorKeyFrame.EasingFunction>
												<ExponentialEase EasingMode="EaseIn"/>
											</EasingColorKeyFrame.EasingFunction>
										</EasingColorKeyFrame>
										<EasingColorKeyFrame KeyTime="0:0:2" Value="#006E97C1"/>
										<EasingColorKeyFrame KeyTime="0:0:3" Value="#4C6E97C1">
											<EasingColorKeyFrame.EasingFunction>
												<ExponentialEase EasingMode="EaseInOut"/>
											</EasingColorKeyFrame.EasingFunction>
										</EasingColorKeyFrame>
									</ColorAnimationUsingKeyFrames>
								</Storyboard>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Unchecked"/>
						<VisualState x:Name="Indeterminate"/>
					</VisualStateGroup>
					<VisualStateGroup x:Name="ValidationStates"/>
				</VisualStateManager.VisualStateGroups>
				<Grid x:Name="Over" Opacity="0" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Ellipse Fill="{TemplateBinding Background}" Margin="1"/>
					<Ellipse StrokeThickness="1" Stroke="{StaticResource CircleIndent}"/>
					<Ellipse x:Name="OverHi" Fill="Transparent" Margin="1"/>
				</Grid>
				<Grid x:Name="Check" Opacity="0" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Ellipse Stroke="{StaticResource CircleCheck}" StrokeThickness="3" Width="34" Height="34" Margin="-2"/>
					<Ellipse x:Name="CheckShine" StrokeThickness="5" Margin="-4">
						<Ellipse.Stroke>
							<RadialGradientBrush Center="0.5,0.5" GradientOrigin="0.5,0.5">
								<GradientStop Color="#7F3A648F" Offset="0.735"/>
								<GradientStop Color="#002E4D6B" Offset="1"/>
								<GradientStop Color="#4C6E97C1" Offset="0.893"/>
							</RadialGradientBrush>
						</Ellipse.Stroke>
					</Ellipse>
				</Grid>
			</Grid>
		</ControlTemplate>
		<Style x:Key="RBStyle" TargetType="RadioButton">
			<Setter Property="Template" Value="{StaticResource RBTemplate}"/>
		</Style>
		
		<ControlTemplate x:Key="TBTemplate" TargetType="ToggleButton">
			<Grid Width="30" Height="30" Margin="5">
				<VisualStateManager.VisualStateGroups>
					<VisualStateGroup x:Name="CommonStates">
						<VisualStateGroup.Transitions>
							<VisualTransition From="Normal" GeneratedDuration="0:0:0.1" To="MouseOver"/>
							<VisualTransition From="MouseOver" GeneratedDuration="0:0:0.2" To="Normal"/>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Normal">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Check">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Disabled">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Check">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.5"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="MouseOver">
							<Storyboard>
								<ColorAnimation Duration="0" To="#26FFFFFF" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="Over" d:IsOptimized="True"/>
								<ColorAnimation Duration="0" To="#0CFFFFFF" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Over" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Pressed"/>
					</VisualStateGroup>
					<VisualStateGroup x:Name="FocusStates"/>
					<VisualStateGroup x:Name="CheckStates">
						<VisualState x:Name="Checked">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Color">
									<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0.9">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
									<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Color">
									<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0.9">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
									<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Unchecked">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="Color">
									<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0.9">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
									<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="Color">
									<EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0.9">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
									<EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
										<EasingDoubleKeyFrame.EasingFunction>
											<ExponentialEase EasingMode="EaseInOut"/>
										</EasingDoubleKeyFrame.EasingFunction>
									</EasingDoubleKeyFrame>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Indeterminate"/>
					</VisualStateGroup>
				</VisualStateManager.VisualStateGroups>
				<Grid x:Name="Check" Opacity="1" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Ellipse x:Name="Bg" Fill="{StaticResource BlackColorBrush}" Margin="1"/>
					<Ellipse x:Name="Color" Fill="{TemplateBinding Background}" Margin="1" RenderTransformOrigin="0.5,0.5">
						<Ellipse.RenderTransform>
							<CompositeTransform/>
						</Ellipse.RenderTransform>
					</Ellipse>
					<Ellipse StrokeThickness="1" Stroke="{StaticResource CircleIndent}"/>
				</Grid>
				<Ellipse x:Name="Over" Margin="1">
					<Ellipse.Fill>
						<RadialGradientBrush GradientOrigin="0.6,0.6" Center="0.6,0.6">
							<GradientStop Color="Transparent" Offset="0"/>
							<GradientStop Color="Transparent" Offset="1"/>
						</RadialGradientBrush>
					</Ellipse.Fill>
				</Ellipse>
			</Grid>
		</ControlTemplate>
		<Style x:Key="TBStyle" TargetType="ToggleButton">
			<Setter Property="Template" Value="{StaticResource TBTemplate}"/>
		</Style>
		
		<ControlTemplate x:Key="MyButtonTemplate" TargetType="Button">
			<Grid Background="Transparent">
				<VisualStateManager.VisualStateGroups>
					<VisualStateGroup x:Name="CommonStates">
						<VisualStateGroup.Transitions>
							<VisualTransition From="Disabled" GeneratedDuration="0:0:0.3" To="Normal"/>
							<VisualTransition From="Normal" GeneratedDuration="0:0:0.1" To="MouseOver"/>
							<VisualTransition From="MouseOver" GeneratedDuration="0:0:0.2" To="Normal"/>
							<VisualTransition From="Normal" GeneratedDuration="0" To="Disabled">
								<Storyboard>
									<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Bg">
										<EasingDoubleKeyFrame KeyTime="0" Value="0"/>
										<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="1"/>
									</DoubleAnimationUsingKeyFrames>
								</Storyboard>
							</VisualTransition>
						</VisualStateGroup.Transitions>
						<VisualState x:Name="Normal">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="MouseOver">
							<Storyboard>
								<ColorAnimation Duration="0" To="#CC475E76" Storyboard.TargetProperty="(Border.Background).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="Bg" d:IsOptimized="True"/>
								<ColorAnimation Duration="0" To="#CC19314D" Storyboard.TargetProperty="(Border.Background).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bg" d:IsOptimized="True"/>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Pressed">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.95"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.95"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
						<VisualState x:Name="Disabled">
							<Storyboard>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="0.5"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="BtnImg">
									<SplineDoubleKeyFrame KeyTime="0" Value="1"/>
								</DoubleAnimationUsingKeyFrames>
							</Storyboard>
						</VisualState>
					</VisualStateGroup>
					<VisualStateGroup x:Name="FocusStates"/>
				</VisualStateManager.VisualStateGroups>
				<Grid x:Name="BtnImg" RenderTransformOrigin="0.5,0.5">
					<Grid.RenderTransform>
						<CompositeTransform/>
					</Grid.RenderTransform>
					<Border x:Name="Bg" CornerRadius="2" BorderThickness="1" BorderBrush="{StaticResource ButtonBorderBrush}">
						<Border.Background>
							<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
								<GradientStop Color="#CC3D5166" Offset="0"/>
								<GradientStop Color="#CC152940" Offset="1"/>
							</LinearGradientBrush>
						</Border.Background>
						<ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"
							noesis:Text.Stroke="#40000000" noesis:Text.StrokeThickness="1" Margin="{TemplateBinding Padding}"/>
					</Border>
				</Grid>
			</Grid>
		</ControlTemplate>
		<Style x:Key="BtnStyle" TargetType="Button">
			<Setter Property="FontSize" Value="16"/>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="FontFamily" Value="Fonts/#Statix"/>
			<Setter Property="Template" Value="{StaticResource MyButtonTemplate}"/>
			<Setter Property="Padding" Value="0,0,0,0"/>
		</Style>
		<SolidColorBrush x:Key="PanelBackgroundBrush" Color="#4C0B1A2B"/>
		<SolidColorBrush x:Key="TransparentColorBrush" Color="Transparent"/>
		<Storyboard x:Name="LogoShineAnim" RepeatBehavior="Forever">
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Shine">
				<EasingDoubleKeyFrame KeyTime="0" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:3" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:3.4" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:3.6" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:4" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:5.5" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:5.9" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:6.1" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:6.5" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:9" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:9.4" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:9.6" Value="1"/>
				<EasingDoubleKeyFrame KeyTime="0:0:10" Value="0"/>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)" Storyboard.TargetName="Shine">
				<EasingDoubleKeyFrame KeyTime="0:0:1" Value="360"/>
				<EasingDoubleKeyFrame KeyTime="0:0:3" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:4" Value="360"/>
				<EasingDoubleKeyFrame KeyTime="0:0:5.5" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:6.5" Value="360"/>
				<EasingDoubleKeyFrame KeyTime="0:0:9" Value="0"/>
				<EasingDoubleKeyFrame KeyTime="0:0:10" Value="360"/>
			</DoubleAnimationUsingKeyFrames>
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="Shine">
